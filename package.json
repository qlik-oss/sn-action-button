    "@babel/core": "7.7.7",
    "@babel/preset-env": "7.7.7",
    "@commitlint/cli": "8.2.0",
    "@commitlint/config-conventional": "8.2.0",
    "@nebula.js/cli": "0.1.0-alpha.28",
