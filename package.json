{
  "name": "@nebula.js/sn-action-button",
  "version": "1.14.3",
  "description": "Supernova action button",
  "license": "MIT",
  "author": "QlikTech International AB",
  "keywords": [
    "qlik",
    "nebula",
    "supernova"
  ],
  "publishConfig": {
    "access": "public"
  },
  "bugs": {
    "url": "https://github.com/qlik-oss/sn-action-button/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/qlik-oss/sn-action-button.git"
  },
  "files": [
    "dist",
    "api-specifications",
    "core"
  ],
  "engines": {
    "node": ">=16"
  },
  "main": "dist/sn-action-button.js",
  "module": "dist/sn-action-button.esm.js",
  "scripts": {
    "build": "node ./tools/build.js --core && shx cp assets/* dist",
    "build:dev": "node ./tools/build.js --core --mode development && shx cp assets/* dist",
    "build:watch": "node ./tools/build.js --mode development -w",
    "lint": "eslint src",
    "start": "nebula serve",
    "sense": "nebula sense",
    "test:unit": "jest",
    "test:integration": "aw puppet --glob 'test/integration/**/*.spec.js'",
    "test:rendering": "aw puppet --testExt '*.spec.js' --glob 'test/rendering/**/*.spec.js' --mocha.bail false --mocha.timeout 30000",
    "prepublishOnly": "NODE_ENV=production yarn run build && yarn spec",
    "prepack": "./tools/prepare-sn-pack.js",
    "spec": "scriptappy-from-jsdoc -c ./spec-configs/props.conf.js"
  },
  "lint-staged": {
    "*.{js, jsx}": [
      "eslint --cache --fix"
    ]
  },
  "peerDependencies": {
    "@nebula.js/stardust": ">=2.2.0"
  },
  "devDependencies": {
    "@after-work.js/aw": "6.0.14",
    "@babel/cli": "7.16.0",
    "@babel/core": "7.16.0",
    "@babel/helper-define-map": "7.16.0",
    "@babel/preset-env": "7.16.4",
    "@commitlint/cli": "12.1.4",
    "@commitlint/config-conventional": "12.1.4",
    "@nebula.js/cli": "2.3.0",
    "@nebula.js/cli-build": "2.3.0",
    "@nebula.js/cli-sense": "2.3.0",
    "@nebula.js/cli-serve": "2.3.0",
    "@nebula.js/stardust": "2.3.0",
    "@nebula.js/test-utils": "0.6.0",
    "babel-plugin-istanbul": "6.1.1",
    "eslint": "7.32.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-plugin-import": "2.25.3",
    "eslint-plugin-mocha": "9.0.0",
    "fs-extra": "^10.0.0",
    "husky": "7.0.4",
    "jest": "^27.3.1",
    "jest-junit": "^13.0.0",
    "lint-staged": "11.2.6",
    "mocha-junit-reporter": "2.0.2",
    "shx": "^0.3.3",
    "yargs": "^17.2.1",
    "scriptappy-from-jsdoc": "0.7.0"
  }
}
