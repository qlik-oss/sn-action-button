    "@nebula.js/test-utils": "0.6.0",
    "babel-plugin-istanbul": "6.0.0",
    "eslint": "7.14.0",
    "eslint-config-airbnb-base": "14.2.1",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-mocha": "8.0.0",
    "husky": "4.3.0",
    "lint-staged": "10.5.2",
    "mocha-junit-reporter": "2.0.0"
